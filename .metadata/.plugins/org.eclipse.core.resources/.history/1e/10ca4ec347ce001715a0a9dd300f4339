package vezba;

public class Stampa {
	
	public static void main(String[] args)
	{
		Stampa obj = new Stampa();
		obj.stampaj();
		obj.saberi(4,5);
		obj.ime("Stefan");
	}
	
    public void stampaj()
    {
    	System.out.println("Hello World");
	}
    public void saberi(int a, int b)
    {
    	int rez = 0;
    	rez = a + b;
    	System.out.println("Zbir " + a + " i " + b + " je " + rez);
    }
    public void ime(String a)
    {
    	System.out.println("Ime ucenika je " +a);
    }
}
