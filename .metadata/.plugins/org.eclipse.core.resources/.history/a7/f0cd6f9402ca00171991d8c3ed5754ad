package vezba;

public class ProveraBroja {
	
	public static void main(String[] args)
	{
		int num = 32;
		
		boolean isPrime = true;
		
		for (int i=2; i<30; i++){
			
		if (num%i==0)
		{
			isPrime = false;
			break;
		}
		}
		if (isPrime)
		{
			System.out.println(num + " is prime!");
		}
		else 
		{
			System.out.println(num + " is not prime!");
		}
	}

}
